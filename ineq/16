  intro sab,
  cases sab, split,
  use succ sab_w, rw add_succ, rw succ_add at sab_h, exact sab_h,
  intro blea,
  cases blea, rw sab_h at blea_h, rw succ_eq_add_one at blea_h,
  rw add_assoc at blea_h, rw ‚Üê add_zero a at blea_h,
  repeat {rw add_assoc at blea_h},
  have cont := add_left_cancel _ _ _ blea_h,
  rw zero_add at cont, rw one_eq_succ_zero at cont, symmetry at cont,
  have cont2 := add_right_eq_zero cont,
  exact succ_ne_zero 0 cont2,